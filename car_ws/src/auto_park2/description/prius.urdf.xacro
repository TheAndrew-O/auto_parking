      </geometry>
    </visual>

    <inertial>
      <mass value="11"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.58631238" ixy="0.0" ixz="0.0" iyy="0.33552910" iyz="0.0" izz="0.33552910"/>
    </inertial>
    <collision name="rear_right_wheel_collision">
      <geometry>
