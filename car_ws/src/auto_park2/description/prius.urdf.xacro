    <collision name="back_bumper">
      <origin xyz="0.0 2.054454 0.577870" rpy="0.0 0 0"/>
      <geometry>
        <box size="1.781650 0.512093 0.581427"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="100.0"/>
      <origin xyz="0 0 0.48" rpy="0 0 1.5708"/>
      <inertia ixx="190.264166667" ixy="0.0" ixz="0.0" 
               iyy="197.576666667" iyz="0.0" 
               izz="44.3141666667"/>
    </inertial>
  </link>
  <!-- front left_wheel -->
  <joint name="wheel_left_joint" type="continuous">
    <parent link="fl_axle"/>
    <child link="wheel_left_link"/>
    <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit effort="1000.0" velocity="10.0" lower="-0.8727" upper="0.8727"/>
  </joint>

  <link name="wheel_left_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file:///home/andrew/auto_parking/car_ws/src/auto_park2/description/wheel.obj" scale="0.01 0.01 0.01"/>
      </geometry>
    </visual>

    <inertial>
      <mass value="11"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.58631238" ixy="0.0" ixz="0.0" iyy="0.33552910" iyz="0.0" izz="0.33552910"/>
    </inertial>
    <collision name="front_left_wheel_collision">
